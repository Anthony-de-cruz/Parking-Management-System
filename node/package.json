{
  "name": "node",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start.dev": "nodemon -L"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pg": "^8.11.5",
    "pug": "2.0.0-beta11",
    "serve-favicon": "2.5.0"
  },
  "devDependencies": {
    "nodemon": "3.1.0"
  },
  "nodemonConfig": {
    "ext": "js,json,ts",
    "watch": [
      "src"
    ],
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "_exec": "ts-node ./src/index.ts",
    "verbose": true,
    "legacyWatch": true,
    "env": {
      "NODE_ENV": "development"
    }
  }
}
